#include<stdio.h>
int main()
{
char str1[10];
char str2[10];

printf("enter string1");
gets(str1);
printf("enter string2");
gets(str2);

int i=0,j=0,k=0;
while(str1[i]!='\0' || str2[i]!='\0')
{
    if(str1[j]==str2[k])
    {
    j++;
    k++;
   
    }
    else
    {
    printf("not equal");
    break;
    }
    i++;
}
if(str1[j]==str2[k])
printf("equal");
}
