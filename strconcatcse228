#include<stdio.h>
int main()
{
char str1[10];
char str2[10];
char str3[20];
printf("enter string1");
gets(str1);
printf("enter string2");
gets(str2);

int i=0,j=0,k=0;
while(str1[i]!='\0')
{
    str3[j]=str1[i];
    i++;
    j++;
    
    
}
i=0;
while(str2[i]!='\0')
{
    str3[j]=str2[i];
    i++;
    j++;
    
    
}
str3[j]='\0';

puts(str3);
}
